{
  "name": "deasilworks/api",
  "type": "library",
  "description": "4klift API Component",
  "homepage": "https://github.com/deasilworks/4klift",
  "license": "MIT",
  "keywords": ["4klift","php","cassandra","cms","api","rest"],
  "authors": [
    {
      "name": "Deasil Works, Inc.",
      "email": "code@deasil.works"
    },
    {
      "name": "Craig Johnston",
      "email": "craig@deasil.works"
    }
  ],
  "require": {
    "php": "^5.6.0",
    "jms/serializer-bundle": ">=0.13.0 <2.0.0"
  },
  "autoload": {
    "psr-4": { "deasilworks\\api\\": "src/" },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "deasilworks\\api\\test\\": "tests/src/deasilworks/api/test"
    }
  },
  "minimum-stability": "dev"
}